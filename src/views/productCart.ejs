<!DOCTYPE html>
<html lang="es">
<head>
    <%- include('./partials/head')%>
    <title>UrbanEats - Carrito de Compras</title>
    <link rel="stylesheet" href="/styles/stylesHome.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    <%- include('./partials/header')%>
    <main>
        <% orden.forEach(orden => { %>
            <ul>
                <img src="/images/products/<%= orden.productos.image %>" alt="">
                <li><%= orden.productos.name %></li>
                <li><%= orden.productos.price %></li>
            </ul>
            <form action="/eliminar/<%= orden.id %>?_method=DELETE" method="POST">
                <button>Eliminar</button>
            </form>
        <% }) %>
    </main>
    <%- include('./partials/footer')%>
</body>
</html>